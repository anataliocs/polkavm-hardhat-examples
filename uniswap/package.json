{
  "name": "uniswap-dapp-pba",
  "version": "0.2.0",
  "scripts": {
    "clear": "rm -rf node_modules/",
    "tidy": "pnpm clear && pnpm prune --no-optional && pnpm store prune && pnpm update",
    "deps": "pnpm install --strict-peer-dependencies --no-optional && pnpm update",
    "reset": "pnpm tidy && pnpm deps",
    "tc": "pnpm hardhat typechain --typecheck",
    "types": "tsc --pretty --strict",
    "compile": "pnpm hardhat compile --typecheck",
    "dev": "pnpm hardhat node-polkadot --show-stack-traces"
  },
  "keywords": [
    "hardhat-polkadot",
    "parity",
    "hardhat"
  ],
  "author": "anataliocs@gmail.com",
  "contributors": [
    "Chris Anatalio"
  ],
  "license": "MIT",
  "description": "",
  "repository": {
    "url": "https://github.com/anataliocs/polkavm-hardhat-examples"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.1.0",
    "@nomicfoundation/hardhat-ethers": "^3.1.0",
    "@nomicfoundation/hardhat-toolbox": "^6.1.0",
    "@parity/hardhat-polkadot": "^0.1.10",
    "@parity/hardhat-polkadot-node": "^0.1.5",
    "@parity/hardhat-polkadot-resolc": "^0.1.8",
    "@typechain/ethers-v6": "^0.5.1",
    "@typechain/hardhat": "^9.1.0",
    "@types/chai": "^4.3.20",
    "dockerode": "^4.0.7",
    "dotenv": "^17.2.1",
    "ethers": "^6.14.0",
    "hardhat": "^2.26.3",
    "polkadot-api": "^1.16.1",
    "run-container": "^2.0.12",
    "solc": "0.8.26",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.8.1",
    "tsx": "^4.20.4"
  }
}
